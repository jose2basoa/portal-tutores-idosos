generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  senha    String
  tutor    Tutor?
}

model Tutor {
  id       Int    @id @default(autoincrement())
  nome     String
  documento String
  idade    Int
  endereco Json
  contatos Json

  userId   Int  @unique
  user     User @relation(fields: [userId], references: [id])
}
